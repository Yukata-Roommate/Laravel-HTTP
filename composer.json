{
    "name": "yukata-roommate/laravel-http",
    "description": "Easy to use HTTP Request and Response for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "autoload": {
        "psr-4": {
            "YukataRm\\Laravel\\Http\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YukataRm\\Laravel\\Http\\Provider\\FacadeServiceProvider"
            ],
            "aliases": {
                "Http": "YukataRm\\Laravel\\Http\\Facade\\Http"
            }
        }
    },
    "authors": [
        {
            "name": "Yukata Roommate",
            "email": "contact@yukata-roommate.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "laravel/framework": "^11.0"
    },
    "minimum-stability": "dev"
}